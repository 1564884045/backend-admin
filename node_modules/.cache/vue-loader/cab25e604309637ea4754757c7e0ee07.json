>\r\n        </router-link>\r\n        <el-submenu v-else :title=\"item.meta.title\" :index=\"resolvePath(item.path)\">\r\n            <template slot=\"title\">\r\n                <svg-icon v-if=\"item.meta.icon\" :name=\"item.meta.icon\" />\r\n                <span>{{ item.meta.title }}</span>\r\n            </template>\r\n            <SidebarItem v-for=\"route in item.children\" :key=\"route.path\" :item=\"route\" :base-path=\"resolvePath(item.path)\" />\r\n        </el-submenu>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport path from 'path'\r\n\r\nexport default {\r\n    name: 'SidebarItem',\r\n    props: {\r\n        item: {\r\n            type: Object,\r\n            required: true\r\n        },\r\n        basePath: {\r\n            type: String,\r\n            default: ''\r\n        }\r\n    },\r\n    data() {\r\n        return {}\r\n    },\r\n    computed: {\r\n        hasChildren() {\r\n            let flag = true\r\n            if (this.item.children) {\r\n                if (this.item.children.every(item => item.meta.sidebar === false)) {\r\n                    flag = false\r\n                }\r\n            } else {\r\n                flag = false\r\n            }\r\n            return flag\r\n        }\r\n    },\r\n    created() {},\r\n    mounted() {},\r\n    methods: {\r\n        isExternal(path) {\r\n            return /^(https?:|mailto:|tel:)/.test(path)\r\n        },\r\n        resolvePath(routePath) {\r\n            if (this.isExternal(routePath)) {\r\n                return routePath\r\n            }\r\n            if (this.isExternal(this.basePath)) {\r\n                return this.basePath\r\n            }\r\n            return path.resolve(this.basePath, routePath)\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n::v-deep .el-menu-item,\r\n::v-deep .el-menu-item span,\r\n::v-deep .el-submenu__title,\r\n::v-deep .el-submenu__title span {\r\n    vertical-align: inherit;\r\n    @include text-overflow;\r\n}\r\